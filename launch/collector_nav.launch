
<launch>
  <master auto="start"/>

  <!-- Run the map server
  <node name="map_server" pkg="map_server" type="map_server" args="$(find scr_proto)/navstack_cfg/testmap.yaml"/>
  -->

  <!--- Run AMCL 
  <include file="$(find amcl)/examples/amcl_diff.launch" />
  -->

  <!--
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find scr_proto)/navstack_cfg/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find scr_proto)/navstack_cfg/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find scr_proto)/navstack_cfg/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find scr_proto)/navstack_cfg/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find scr_proto)/navstack_cfg/base_local_planner_params.yaml" command="load" />
  </node>
  -->
  
  <node name="kinect_laser" type="depthimage_to_laserscan" pkg="depthimage_to_laserscan" args="image:=/camera/depth/image_raw">
  </node>

  <node pkg="gmapping" type="slam_gmapping" name="slam_gmapping" output="screen">
    <!--remap from="scan" to="base_scan"/-->
    <param name="odom_frame" value="odom"/>
    <param name="map_update_interval" value="5.0"/>
    <param name="maxUrange" value="4.0"/> <!-- The maximum usable range of the laser. A beam is cropped to this value. -->
    <param name="maxRange" value="4.0"/> <!-- The maximum range of the sensor. If regions with no obstacles within the range of the sensor should appear as free space in the map, set maxUrange < maximum range of the real sensor <= maxRange.-->
    <param name="sigma" value="0.05"/>
    <param name="kernelSize" value="1.0"/>
    <param name="lstep" value="0.05"/>
    <param name="astep" value="0.05"/>
    <param name="iterations" value="5"/>
    <param name="lsigma" value="0.075"/>
    <param name="ogain" value="3.0"/>
    <param name="lskip" value="0"/>
    <param name="srr" value="0.03"/>
    <param name="srt" value="0.06"/>
    <param name="str" value="0.03"/>
    <param name="stt" value="0.06"/>
    <param name="linearUpdate" value="0.3"/> <!-- Process a scan each time the robot translates this far -->
    <param name="angularUpdate" value="0.3"/> <!--Process a scan each time the robot rotates this far; 0.1 represents 5.7 degress -->
    <param name="temporalUpdate" value="-1.0"/>
    <param name="resampleThreshold" value="0.5"/>
    <param name="particles" value="200"/>
    <param name="xmin" value="-25.0"/>
    <param name="ymin" value="-25.0"/>
    <param name="xmax" value="25.0"/>
    <param name="ymax" value="25.0"/>
    <param name="delta" value="0.05"/>
    <param name="llsamplerange" value="0.01"/>
    <param name="llsamplestep" value="0.01"/>
    <param name="lasamplerange" value="0.005"/>
    <param name="lasamplestep" value="0.005"/>
  </node>

</launch>

